{
  "name": "@arcadia-eternity/server",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/arcadia-eternity/arcadia-eternity.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "run-p build:code build:types",
    "clear": "rm -rf dist",
    "build:code": "rollup -c",
    "watch:code": "rollup -c --watch",
    "watch:types": "tsc --emitDeclarationOnly --watch --preserveWatchOutput",
    "watch": "run-p watch:code watch:types",
    "build:types": "tsc --emitDeclarationOnly",
    "generate-proto": "protoc --plugin=protoc-gen-ts_proto=../../node_modules/.pnpm/node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated --ts_proto_opt=esModuleInterop=true,forceLong=string,useOptionals=messages --proto_path=./proto ./proto/battle-rpc.proto",
    "start": "node dist/index.js",
    "dev": "npm run build && npm start",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [],
  "author": "yuuinih",
  "license": "MIT",
  "dependencies": {
    "@arcadia-eternity/battle": "workspace:^",
    "@arcadia-eternity/const": "workspace:^",
    "@arcadia-eternity/database": "workspace:^",
    "@arcadia-eternity/parser": "workspace:^",
    "@arcadia-eternity/protocol": "workspace:^",
    "@arcadia-eternity/rules": "workspace:^",
    "@arcadia-eternity/schema": "workspace:^",
    "@aws-sdk/client-sesv2": "^3.844.0",
    "@bufbuild/protobuf": "^2.6.2",
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@types/handlebars": "^4.1.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "handlebars": "^4.7.8",
    "inversify": "^7.5.4",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.1.5",
    "nodemailer": "^7.0.5",
    "pino": "^9.7.0",
    "prom-client": "^15.1.3",
    "redis": "^5.6.0",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.13",
    "@types/nodemailer": "^6.4.17",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "rollup-plugin-copy": "^3.5.0",
    "ts-proto": "^2.7.5"
  }
}
